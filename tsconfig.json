{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "component/Form/Forms/Location/LocationForms.js", "component/Form/Forms/Time/Time.js", "component/Form/Forms/Education.js", "component/Form/Forms/Subject/SubjectsForms.js", "component/Form/Form.js"],
  "exclude": ["node_modules"]
}
